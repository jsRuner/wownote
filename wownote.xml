<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"> 
<Frame name="WowNoteFrame" parent="UIParent" hidden="true" enableMouse="true" movable="true"> 
   <TitleRegion setAllPoints="true"/>
   <Size x="400" y="250" /> 
   <Anchors> 
      <Anchor point="CENTER" /> 
   </Anchors> 
   <Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border"> 
      <BackgroundInsets> 
         <AbsInset left="11" right="12" top="12" bottom="11" /> 
      </BackgroundInsets> 
   </Backdrop> 
   <Layers> 
      <Layer level="ARTWORK"> 
         <FontString name="$parentTextFPS" inherits="GameFontNormal" text="10年魔兽，爱恨情仇。影响我人生的游戏"> 
            <Anchors> 
               <Anchor point="TOPLEFT" relativeTo="WowNoteFrame" > 
                <Offset x="15" y="-15" /> 
                 </Anchor> 
            </Anchors> 
         </FontString> 

           <FontString name="$parentTextDelay" inherits="GameFontNormal" text="今晚走出了第一步,我的第一个魔兽世界插件。"> 
            <Anchors> 
               <Anchor point="TOPLEFT" relativeTo="WowNoteFrame" > 
                <Offset x="15" y="-30" /> 
                 </Anchor> 
            </Anchors> 
         </FontString>

           <FontString  name="$parentTextMoney" inherits="GameFontNormal" text="这一刻是过去游戏以玩为主的终点，现在是研究背后技术的起点"> 
            <Anchors> 
               <Anchor point="TOPLEFT" relativeTo="WowNoteFrame" > 
                <Offset x="15" y="-45" /> 
                 </Anchor> 
            </Anchors> 
         </FontString>

          <FontString  name="$parentTextNote"  inherits="GameFontNormal" text=""> 
            <Anchors> 
               <Anchor point="TOPLEFT" relativeTo="WowNoteFrame" > 
                <Offset x="15" y="-60" /> 
                 </Anchor> 
            </Anchors> 
         </FontString>


         <FontString inherits="GameFontNormal" size="20" text="吴文付"> 
            <Anchors> 
               <Anchor point="CENTER" relativeTo="WowNoteFrame" > 
                 </Anchor> 
            </Anchors> 
         </FontString>
      </Layer> 
   </Layers> 

    <Scripts> 
      <OnLoad> 
      Wownote_Load(); 
      </OnLoad> 
      <OnUpdate> 
      Wownote_Update(); 
      </OnUpdate> 

      <OnEvent> 
   Wownote_Event(); 
</OnEvent>


   </Scripts> 


</Frame> 
</Ui>